{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },

  "env": {
    "es6": true,
    "node": true,
    "jasmine": true
  },

  "plugins": [
    "standard",
    "promise"
  ],

  "globals": {
    "document": false,
    "navigator": false,
    "window": false, 
    "angular": true
  },

  "rules": {
"no-unused-vars": [0, { "varsIgnorePattern": "^h$" }]
  },

"extends": "eslint:recommended"
}
